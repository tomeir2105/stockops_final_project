            "uid": "bf0qvrajzfzeof"
          },
          "query": "import \"math\"\r\n\r\nfrom(bucket: \"netdata_2h\")\r\n  |> range(start: -2h)\r\n  |> filter(fn: (r) =>\r\n    r._measurement == \"netdata_disk\" and\r\n    (r._field == \"used_gib\" or r._field == \"free_gib\")\r\n  )\r\n  |> group(columns: [\"host\", \"_field\", \"mount\"])\r\n  |> last()\r\n  |> group(columns: [\"host\", \"_field\"])\r\n  |> sum(column: \"_value\")\r\n  |> pivot(rowKey: [\"host\"], columnKey: [\"_field\"], valueColumn: \"_value\")\r\n  |> map(fn: (r) => ({\r\n      _time: now(),\r\n      host: r.host,\r\n      used_gib:  math.round(x: r.used_gib * 100.0) / 100.0,\r\n      free_gib:  math.round(x: r.free_gib * 100.0) / 100.0,\r\n      total_gib: math.round(x: (r.used_gib + r.free_gib) * 100.0) / 100.0,\r\n      used_percent: (r.used_gib / (r.used_gib + r.free_gib)) * 100.0\r\n  }))\r\n  |> keep(columns: [\"_time\",\"host\",\"used_percent\",\"used_gib\",\"free_gib\",\"total_gib\"])\r\n  |> sort(columns: [\"host\"])\r\n  |> group()\r\n",
          "refId": "A"
        }
      ],
